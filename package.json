{
  "name": "edgerelay",
  "version": "0.0.0",
  "private": true,
  "description": "AT Protocol Relay implementation for Cloudflare Workers",
  "type": "module",
  "scripts": {
    "dev": "wrangler dev",
    "deploy": "wrangler deploy",
    "types": "wrangler types",
    "lint": "eslint --ext .ts,.tsx src/",
    "lint:fix": "eslint --ext .ts,.tsx src/ --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,json}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,json}\"",
    "test": "vitest run",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "atproto",
    "at-protocol",
    "bluesky",
    "relay",
    "firehose",
    "jetstream",
    "cloudflare",
    "workers",
    "durable-objects"
  ],
  "author": "Taylor Marvin",
  "license": "MIT",
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20241127.0",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "prettier": "^3.2.5",
    "typescript": "^5.3.3",
    "vite": "^5.1.0",
    "vitest": "^1.3.0",
    "wrangler": "^3.85.0"
  },
  "dependencies": {
    "@atproto/api": "^0.13.0",
    "@atproto/xrpc": "^0.6.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
